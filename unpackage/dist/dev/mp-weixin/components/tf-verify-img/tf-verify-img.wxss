.tf-Box-Bg {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 100;
  background-color: rgba(0, 0, 0, 0.3);
  display: flex;
  align-items: center;
  justify-content: center;
}
.tf-Box-Bg .tf-Box {
  width: 640rpx;
  height: 584rpx;
  background-color: #fff;
  border-radius: 6rpx;
  box-shadow: 0 0 50rpx 0rpx rgba(0, 0, 0, 0.2);
}
.tf-Box-Bg .tf-Box .tf-Box-title {
  height: 100rpx;
  line-height: 1;
  padding: 0 32rpx;
  font-size: 32rpx;
  border-bottom: 1px solid #E1E3E9;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.tf-Box-Bg .tf-Box .tf-Box-title .tf-close {
  width: 28rpx;
  height: 28rpx;
  background-image: url(data:image/png;base64,);
  background-size: 100% 100%;
}
.tf-Box-Bg .tf-Box .tf-Box-center {
  margin: 30rpx auto;
  border-radius: 6rpx;
}
.tf-Box-Bg .tf-Box .tf-Box-BtnBox {
  margin: 30rpx auto;
  width: 580rpx;
  height: 75rpx;
  line-height: 75rpx;
  text-align: center;
  font-size: 28rpx;
  border-radius: 6rpx;
  border: 1px solid #E1E3E9;
  background-color: #F7F8F9;
  overflow: hidden;
  position: relative;
}
.tf-Box-Bg .tf-Box .tf-Box-BtnBox .tf-Box-BtnBox-text {
  width: 100%;
  height: 75rpx;
  position: absolute;
  top: 0;
  left: 0;
  color: #424649;
  text-align: center;
}
.tf-Box-Bg .tf-Box .tf-Box-BtnBox .tf-Box-BtnNei {
  height: 75rpx;
  width: 75rpx;
  background-color: #fff;
  box-shadow: 0 0 10rpx 0rpx rgba(0, 0, 0, 0.2);
  background-image: url(data:image/png;base64,);
  background-size: 34rpx;
  background-position: center;
  background-repeat: no-repeat;
}
.tf-Box-Bg .tf-Box .tf-Box-BtnBox .tf-Box-BtnNei .tf-Box-BtnNei-leftBox {
  position: absolute;
  top: 0;
  left: -580rpx;
  width: 580rpx;
  height: 100%;
}

